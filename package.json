{
	"author": {
		"name": "Baptiste Pillot",
		"email": "baptiste@pillot.fr"
	},
	"dependencies": {
		"@itrocks/class-type": "latest",
		"typescript": "~5.6"
	},
	"description": "Runtime parameter name reflection from TypeScript declaration files",
	"devDependencies": {
		"@types/node": "^22.10"
	},
	"engines": {
		"node": ">=18"
	},
	"exports": {
		"import": "./esm/parameter-name.js",
		"require": "./cjs/parameter-name.js"
	},
	"files": [
		"LICENSE",
		"README.md",
		"cjs/*",
		"esm/*",
		"!*.map"
	],
	"homepage": "https://it.rocks",
	"keywords": [
		"backend",
		"constructor",
		"function",
		"it.rocks",
		"method",
		"name",
		"parameter",
		"parameters",
		"parse",
		"parsing",
		"reflection",
		"utility"
	],
	"license": "ISC",
	"name": "@itrocks/parameter-name",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/itrocks-ts/parameter-name.git"
	},
	"scripts": {
		"build": "npm run build:cjs && npm run build:esm",
		"build:cjs": "tsc -p tsconfig.cjs.json",
		"build:esm": "tsc -p tsconfig.esm.json"
	},
	"types": "./esm/parameter-name.d.ts",
	"version": "0.0.1"
}
